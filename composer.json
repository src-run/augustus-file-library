{
  "name": "src-run/augustus-storage-object-library",
  "license": "MIT",
  "type": "library",
  "homepage": "https://src.run/augustus-file-library/home",
  "description": "An extended SplFileInfo that includes additional attribute helpers and temporary file auto removal.",
  "support": {
    "email": "rmf@src.run",
    "source": "https://src.run/augustus-file-library/git",
    "issues": "https://src.run/augustus-file-library/issues"
  },
  "keywords": [ "augustus-file-library", "augustus", "file", "library", "rmf", "src-run" ],
  "authors": [
    {
      "name": "Rob Frawley 2nd",
      "email": "rmf@src.run",
      "homepage": "https://src.run/rmf",
      "role": "Project Lead"
    }
  ],
  "require": {
    "php": "~7.1",
    "ramsey/uuid": "~3.7",
    "src-run/augustus-exception-library": "~0.8,>=0.8.4",
    "src-run/augustus-silencer-library": "~0.8,>=0.8.1",
    "symfony/lock": "~3.0 | ~4.0",
    "symfony/process": "~3.3 | ~4.0"
  },
  "require-dev": {
    "ext-fileinfo": "*",
    "codacy/coverage": "~1.0",
    "friendsofphp/php-cs-fixer": "~2.0",
    "mikey179/vfsStream": "~1.6.5",
    "satooshi/php-coveralls": "~2.0",
    "symfony/finder": "~4.0",
    "symfony/phpunit-bridge": "~4.0"
  },
  "suggest": {
    "ext-fileinfo": "Required to use the fileinfo-based mime-type resolver."
  },
  "autoload": {
    "psr-4": {
      "SR\\File\\": "lib"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SR\\File\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "bin-dir": "bin",
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.8-dev"
    }
  }
}
